<?xml version="1.0" encoding="UTF-8"?>
<!-- Mit XMLSpy v2013 rel. 2 sp2 (x64) (http://www.altova.com) von XMLSpy2013 (s IT Solutions AT Spardat GmbH z.B. Buchhaltung) bearbeitet -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:include schemaLocation="FundsXML4_Core.xsd"/>
	<xs:include schemaLocation="FundsXML4_TransactionData.xsd"/>
	<xs:complexType name="PortfolioType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Complete portfolio data including positions, transactions, and breakdowns</xs:documentation>
			<xs:documentation xml:lang="de">[AUTO] Vollständige Portfolio-Daten einschließlich Positionen, Transaktionen und Aufschlüsselungen</xs:documentation>
			<xs:documentation xml:lang="fr">Complete portefeuille données y compris positions, transactions, et breakdowns</xs:documentation>
			<xs:documentation xml:lang="nl">Complete portefeuille gegevens inclusief positions, transactions, en breakdowns</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NavDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="en">NAV date for all data within this section</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] NAV-Datum für alle Daten in diesem Abschnitt</xs:documentation>
					<xs:documentation xml:lang="fr">NAV date pour tous données within ce section</xs:documentation>
					<xs:documentation xml:lang="nl">NAV datum voor alle gegevens within dit sectie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Positions" type="PositionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">All positions/holdings of fund/portfolio (including accounts, earnings and fees)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Alle Positionen/Bestände des Fonds/Portfolios (einschließlich Konten, Erträge und Gebühren)</xs:documentation>
					<xs:documentation xml:lang="fr">All positions/détentions de fonds/portefeuille (y compris accounts, earnings et fees)</xs:documentation>
					<xs:documentation xml:lang="nl">All positions/bezittingen van fonds/portefeuille (inclusief accounts, earnings en fees)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Transactions" type="TransactionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Transactions within the portfolio (securities and other assets bought or sold)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Transaktionen innerhalb des Portfolios (gekaufte oder verkaufte Wertpapiere und andere Vermögenswerte)</xs:documentation>
					<xs:documentation xml:lang="fr">Transactions within portefeuille (titres et autre assets bought ou sold)</xs:documentation>
					<xs:documentation xml:lang="nl">Transactions within portefeuille (effecten en overig assets bought van sold)</xs:documentation>
				</xs:annotation>
				<xs:key name="transactionID">
					<xs:selector xpath="Transaction"/>
					<xs:field xpath="TransactionID"/>
				</xs:key>
			</xs:element>
			<xs:element name="Earnings" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Earnings data (coupons, dividends, distributions) resulting from security positions</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Ertragsdaten (Coupons, Dividenden, Ausschüttungen) aus Wertpapierpositionen</xs:documentation>
					<xs:documentation xml:lang="fr">Earnings données (coupons, dividends, distributions) resulting de titre positions</xs:documentation>
					<xs:documentation xml:lang="nl">Earnings gegevens (coupons, dividends, distributions) resulting van effect positions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="EarningsType">
							<xs:attribute name="from" type="xs:date"/>
							<xs:attribute name="to" type="xs:date"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PositionsDecomposed" type="PositionsDecomposedType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Decomposed positions (holdings of share classes are replaced by the scaled down positions of the subfund)</xs:documentation>
					<xs:documentation xml:lang="de">Aufgelöste Bestände (Anteilscheinbestände werden durch die anteiligen Bestände des Subfonds ersetzt)</xs:documentation>
					<xs:documentation xml:lang="fr">Decomposed positions (détentions de action classes sont replaced par scaled down positions de subfund)</xs:documentation>
					<xs:documentation xml:lang="nl">Decomposed positions (bezittingen van aandeel classes zijn replaced door scaled down positions van subfund)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BreakDowns" type="BreakDownsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">All kind of break down data (including currency exposure)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Alle Arten von Aufschlüsselungsdaten (einschließlich Währungsexposure)</xs:documentation>
					<xs:documentation xml:lang="fr">All kind de break down données (y compris devise exposition)</xs:documentation>
					<xs:documentation xml:lang="nl">All kind van break down gegevens (inclusief valuta blootstelling)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RiskCodes" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">List of risk codes (risk indicators, key risk figures)</xs:documentation>
					<xs:documentation xml:lang="de">Risiko-Kennzahlen</xs:documentation>
					<xs:documentation xml:lang="fr">Liste de risque codes (risque indicators, key risque figures)</xs:documentation>
					<xs:documentation xml:lang="nl">Lijst van risico codes (risico indicators, key risico figures)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RiskCode" type="RiskCodeType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation xml:lang="en">Risk code value</xs:documentation>
								<xs:documentation xml:lang="de">[AUTO] Risikokennzahl-Wert</xs:documentation>
								<xs:documentation xml:lang="fr">Risque code valeur</xs:documentation>
								<xs:documentation xml:lang="nl">Risico code waarde</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PositionType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Details of a single position in the portfolio</xs:documentation>
			<xs:documentation xml:lang="de">[AUTO] Details einer einzelnen Position im Portfolio</xs:documentation>
			<xs:documentation xml:lang="fr">Details de un unique position dans portefeuille</xs:documentation>
			<xs:documentation xml:lang="nl">Details van een enkel positie in portefeuille</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="UniqueID">
				<xs:annotation>
					<xs:documentation xml:lang="en">Unique identifier referencing the corresponding element in the AssetMasterData block</xs:documentation>
					<xs:documentation xml:lang="de">Eindeutige Referenznummer der Stammdaten der Position (muss unter /AssetMasterData/Asset/UniqueID vorhanden sein)</xs:documentation>
					<xs:documentation xml:lang="fr">Unique identifiant referencing corresponding élément dans AssetMasterData block</xs:documentation>
					<xs:documentation xml:lang="nl">Uniek identificatie referencing corresponding element in AssetMasterData block</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:IDREF">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Identifiers" type="IdentifiersType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifiers of instrument (like ISIN, Ticker, ...)
</xs:documentation>
					<xs:documentation xml:lang="en">Kennnummer des im Portfolio enthaltenen Instrumentes (ISIN, Ticker, ...)</xs:documentation>
					<xs:documentation xml:lang="fr">Kennnummer des im Portefeuille enthaltenen Instrumentes (ISIN, Ticker, ...)</xs:documentation>
					<xs:documentation xml:lang="nl">Kennnummer des im Portefeuille enthaltenen Instrumentes (ISIN, Ticker, ...)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="ISOCurrencyCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Valuation currency (original currency of security/derivative/cash). For FX Forward, this is the Base CCY.</xs:documentation>
					<xs:documentation xml:lang="de">Bewertungswährung (Währung des Wertpapiers/Derivates/Geldes). Für FX Forwards enthällt dieses Feld die Base CCY.</xs:documentation>
					<xs:documentation xml:lang="fr">Évaluation devise (original devise de titre/dérivé/cash). For FX Forward, ce est Base CCY.</xs:documentation>
					<xs:documentation xml:lang="nl">Waardering valuta (original valuta van effect/derivaat/cash). For FX Forward, dit is Base CCY.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalValue" type="FundAmountType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Total value of position (including interests ...). Sum of all total values in fund currency must be equal to fund volume (total NAV of fund)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Gesamtwert der Position (einschließlich Zinsen...). Die Summe aller Gesamtwerte in Fondswährung muss dem Fondsvolumen (Gesamt-NAV des Fonds) entsprechen</xs:documentation>
					<xs:documentation xml:lang="fr">Total valeur de position (y compris interests ...). Sum de tous total values dans fonds devise must be equal à fonds volume (total NAV de fonds)</xs:documentation>
					<xs:documentation xml:lang="nl">Totaal waarde van positie (inclusief interests ...). Sum van alle totaal values in fonds valuta must be equal naar fonds volume (totaal NAV van fonds)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherTotalValues" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Additional total values (e.g. Hold-To-Maturity)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Zusätzliche Gesamtwerte (z.B. Hold-To-Maturity)</xs:documentation>
					<xs:documentation xml:lang="fr">Supplémentaire total values (e.g. Hold-To-Maturity)</xs:documentation>
					<xs:documentation xml:lang="nl">Aanvullend totaal values (e.g. Hold-To-Maturity)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OtherTotalValue" type="OtherTotalValueType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TotalPercentage" type="PercentageType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Percentage of total value based on total asset value (of fund or share class)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Prozentualer Anteil am Gesamtvermögen (des Fonds oder der ShareClass)</xs:documentation>
					<xs:documentation xml:lang="fr">Pourcentage de total valeur based on total actif valeur (de fonds ou classe d'actions)</xs:documentation>
					<xs:documentation xml:lang="nl">Percentage van totaal waarde based on totaal actief waarde (van fonds van aandelenklasse)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AveragePurchasePrice" type="FundAmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Average purchase price (considering all transactions related to this position)</xs:documentation>
					<xs:documentation xml:lang="de">Mischeinstands-Preis</xs:documentation>
					<xs:documentation xml:lang="fr">Average purchase prix (considering tous transactions related à ce position)</xs:documentation>
					<xs:documentation xml:lang="nl">Average purchase prijs (considering alle transactions related naar dit positie)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AveragePurchaseFXRate" type="FXRatesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Average purchase fx rates (considering all transactions related to this position)</xs:documentation>
					<xs:documentation xml:lang="de">Mischeinstands-Devisenkurs</xs:documentation>
					<xs:documentation xml:lang="fr">Average purchase fx rates (considering tous transactions related à ce position)</xs:documentation>
					<xs:documentation xml:lang="nl">Average purchase fx rates (considering alle transactions related naar dit positie)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Exposures" type="ExposureType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Exposure of position (for different approaches)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Exposure der Position (für verschiedene Ansätze)</xs:documentation>
					<xs:documentation xml:lang="fr">Exposition de position (pour different approaches)</xs:documentation>
					<xs:documentation xml:lang="nl">Blootstelling van positie (voor different approaches)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FXRates" type="FXRatesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">FX rates used for evaluation of position</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Für die Positionsbewertung verwendete Wechselkurse</xs:documentation>
					<xs:documentation xml:lang="fr">FX rates used pour evaluation de position</xs:documentation>
					<xs:documentation xml:lang="nl">FX rates used voor evaluation van positie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PriceDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Date of pricing information taken for fund valuation</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Datum der Preisinformation für die Fondsbewertung</xs:documentation>
					<xs:documentation xml:lang="fr">Date de pricing information taken pour fonds évaluation</xs:documentation>
					<xs:documentation xml:lang="nl">Datum van pricing informatie taken voor fonds waardering</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PricingSource" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Pricing source (including OTC positions)</xs:documentation>
					<xs:documentation xml:lang="de">Gibt die Kursquelle an (auch für OTC-Positionen)</xs:documentation>
					<xs:documentation xml:lang="fr">Pricing source (y compris OTC positions)</xs:documentation>
					<xs:documentation xml:lang="nl">Pricing source (inclusief OTC positions)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="Listed">
							<xs:annotation>
								<xs:documentation xml:lang="en">Pricing source with enumeration list</xs:documentation>
								<xs:documentation xml:lang="de">Pricing source mit enumeration Liste</xs:documentation>
								<xs:documentation xml:lang="fr">Pricing source avec enumeration liste</xs:documentation>
								<xs:documentation xml:lang="nl">Pricing source met enumeration lijst</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="BARCLAYS"/>
									<xs:enumeration value="BLOOMBERG"/>
									<xs:enumeration value="BROKERS"/>
									<xs:enumeration value="FACTSET"/>
									<xs:enumeration value="JPMORGAN"/>
									<xs:enumeration value="LPC"/>
									<xs:enumeration value="MARKIT"/>
									<xs:enumeration value="MERRILL"/>
									<xs:enumeration value="NTRS"/>
									<xs:enumeration value="REUTERS"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Unlisted" type="Text200Type">
							<xs:annotation>
								<xs:documentation xml:lang="en">Free text pricing source</xs:documentation>
								<xs:documentation xml:lang="de">[AUTO] Freitextangabe zur Kursquelle</xs:documentation>
								<xs:documentation xml:lang="fr">Free text pricing source</xs:documentation>
								<xs:documentation xml:lang="nl">Free text pricing source</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:choice>
				<xs:element name="Equity">
					<xs:annotation>
						<xs:documentation xml:lang="en">Details for equity positions</xs:documentation>
						<xs:documentation xml:lang="de">Details für Aktienpositionen</xs:documentation>
						<xs:documentation xml:lang="fr">Details pour action positions</xs:documentation>
						<xs:documentation xml:lang="nl">Details voor aandeel positions</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Units" type="xs:decimal">
								<xs:annotation>
									<xs:documentation xml:lang="en">Units of equity</xs:documentation>
									<xs:documentation xml:lang="de">Stücke der Aktie im Bestand</xs:documentation>
									<xs:documentation xml:lang="fr">Units de action</xs:documentation>
									<xs:documentation xml:lang="nl">Units van aandeel</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Price" type="FundAmountType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Price (in different currencies)</xs:documentation>
									<xs:documentation xml:lang="de">Preis der Aktie (in verschiedenen Währungen)</xs:documentation>
									<xs:documentation xml:lang="fr">Prix (dans different currencies)</xs:documentation>
									<xs:documentation xml:lang="nl">Prijs (in different currencies)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="OtherPrices" type="OtherPricesType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Additional prices (e.g. hold to maturity)</xs:documentation>
									<xs:documentation xml:lang="de">Zusätzlich prices (e.g. hold zu maturity)</xs:documentation>
									<xs:documentation xml:lang="fr">Supplémentaire prices (e.g. hold à maturity)</xs:documentation>
									<xs:documentation xml:lang="nl">Aanvullend prices (e.g. hold naar maturity)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MarketValue" type="FundAmountType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Market value of position (excluding dividends)</xs:documentation>
									<xs:documentation xml:lang="de">Kurswert der Aktienposition (exklusive Ansprüche)</xs:documentation>
									<xs:documentation xml:lang="fr">Valeur de marché de position (à l'exclusion de dividends)</xs:documentation>
									<xs:documentation xml:lang="nl">Marktwaarde van positie (exclusief dividends)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="BookRate" type="FundAmountType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Book rate</xs:documentation>
									<xs:documentation xml:lang="de">Buchkurs</xs:documentation>
									<xs:documentation xml:lang="fr">Book taux</xs:documentation>
									<xs:documentation xml:lang="nl">Book tarief</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PurchaseValue" type="FundAmountType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Purchase value of position</xs:documentation>
									<xs:documentation xml:lang="de">Einstandswert der Position</xs:documentation>
									<xs:documentation xml:lang="fr">Purchase valeur de position</xs:documentation>
									<xs:documentation xml:lang="nl">Purchase waarde van positie</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DividendsDue" type="FundAmountType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Dividends due of position</xs:documentation>
									<xs:documentation xml:lang="de">Dividendenforderungen fuer Aktienbestaende</xs:documentation>
									<xs:documentation xml:lang="fr">Dividends due de position</xs:documentation>
									<xs:documentation xml:lang="nl">Dividends due van positie</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LendedFlag" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Flag indicating that part of position is lended</xs:documentation>
									<xs:documentation xml:lang="de">Kennzeichen, dass Teile des Bestandes verliehen wurden</xs:documentation>
									<xs:documentation xml:lang="fr">Indicateur indicating que part de position est lended</xs:documentation>
									<xs:documentation xml:lang="nl">Vlag indicating dat part van positie is lended</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LendedUnits" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Number of lended equities</xs:documentation>
									<xs:documentation xml:lang="de">Anzahl der verliehenen Stücke</xs:documentation>
									<xs:documentation xml:lang="fr">Numéro de lended equities</xs:documentation>
									<xs:documentation xml:lang="nl">Nummer van lended equities</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="IndicatorRaffled" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Indicates whether security has been raffled</xs:documentation>
									<xs:documentation xml:lang="de">Gibt an, ob das Wertpapier verlost wurde</xs:documentation>
									<xs:documentation xml:lang="fr">Indicates whether titre un been raffled</xs:documentation>
									<xs:documentation xml:lang="nl">Indicates whether effect heeft been raffled</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Bond">
					<xs:annotation>
						<xs:documentation xml:lang="en">Details for bond positions</xs:documentation>
						<xs:documentation xml:lang="de">[AUTO] Details für Anleihe-Positionen</xs:documentation>
						<xs:documentation xml:lang="fr">Details pour obligation positions</xs:documentation>
						<xs:documentation xml:lang="nl">Details voor obligatie positions</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Nominal" type="xs:decimal">
								<xs:annotation>
									<xs:documentation xml:lang="en">Nominal value (units) of bond</xs:documentation>
									<xs:documentation xml:lang="de">Nominale/Stückzahl der Rentenposition</xs:documentation>
									<xs:documentation xml:lang="fr">Nominal valeur (units) de obligation</xs:documentation>
									<xs:documentation xml:lang="nl">Nominal waarde (units) van obligatie</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="OriginalFace" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Par value of a bond when it has been bought</xs:documentation>
									<xs:documentation xml:lang="de">[AUTO] Nennwert einer Anleihe zum Kaufzeitpunkt</xs:documentation>
									<xs:documentation xml:lang="fr">Par valeur de un obligation quand it un been bought</xs:documentation>
									<xs:documentation xml:lang="nl">Par waarde van een obligatie wanneer it heeft been bought</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Price" type="FundAmountType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Clean price (in different currencies)</xs:documentation>
									<xs:documentation xml:lang="de">Bewertungskurs (in verschiedenen Währungen)</xs:documentation>
									<xs:documentation xml:lang="fr">Prix pied de coupon (dans different currencies)</xs:documentation>
									<xs:documentation xml:lang="nl">Schone prijs (in different currencies)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="OtherPrices" type="OtherPricesType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Additional prices (e.g. hold to maturity)</xs:documentation>
									<xs:documentation xml:lang="de">[AUTO] Zusätzliche Preise (z.B. Hold-To-Maturity)</xs:documentation>
									<xs:documentation xml:lang="fr">Supplémentaire prices (e.g. hold à maturity)</xs:documentation>
									<xs:documentation xml:lang="nl">Aanvullend prices (e.g. hold naar maturity)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DirtyPrice" type="FundAmountType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="en">Dirty Price (including interests)</xs:documentation>
