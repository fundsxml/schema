						<xs:enumeration value="Custom"/>
						<xs:enumeration value="Absolute Value/Target Return"/>
						<xs:enumeration value="Peer Groups and Universes"/>
						<xs:enumeration value="Factor Based Models"/>
						<xs:enumeration value="Returns Based"/>
