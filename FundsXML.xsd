			<xs:element name="RiskAndReturnProfile">
				<xs:annotation>
					<xs:documentation xml:lang="en">Art.8</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Heading" type="xs:string">
