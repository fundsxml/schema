         <xs:element minOccurs="0" name="IsLeveragedFlag" type="xs:boolean"/>
         <xs:element minOccurs="0" name="IsReferenceShareClass" type="xs:boolean"/>
         <xs:element minOccurs="0" name="IsPublicFlag" type="xs:boolean">
