                                 <xs:documentation>Actual amount of Basis</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="FeeAsPercentageOfBasis" type="PercentageType"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
