							<xs:annotation>
								<xs:documentation>Net Assets </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NetSales" type="xs:decimal">
							<xs:annotation>
								<xs:documentation>Net sales for a certain period</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Segmentation">
							<xs:annotation>
								<xs:documentation>Information about the segmentation of funds</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="FundsWithoutSegments" type="xs:int">
