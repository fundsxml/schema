														<xs:documentation xml:lang="de">[AUTO] Prozentualer Anteil am Gesamtportfolio</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="BreakDowns" type="BreakDownsType" minOccurs="0">
													<xs:annotation>
														<xs:documentation xml:lang="en">Optional breakdowns on next level</xs:documentation>
														<xs:documentation xml:lang="de">[AUTO] Optionale Aufschlüsselungen auf der nächsten Ebene</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OtherTotalValueType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Alternative total value with type specification</xs:documentation>
			<xs:documentation xml:lang="de">[AUTO] Alternativer Gesamtwert mit Typangabe</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">Type of total value (e.g. Hold-To-Maturity)</xs:documentation>
					<xs:documentation xml:lang="de">[AUTO] Art des Gesamtwerts (z.B. Hold-To-Maturity)</xs:documentation>
