               <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="Distribution" type="DistributionType"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element minOccurs="0" name="Fees" type="FeesType">
