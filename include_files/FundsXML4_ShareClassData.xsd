								<xs:sequence>
									<xs:element name="Language" type="xs:language"/>
									<xs:element name="Description" type="xs:string" minOccurs="0"/>
									<xs:element name="Caption" type="xs:string" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
