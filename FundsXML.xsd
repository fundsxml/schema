										<xs:enumeration value="SD"/>
										<xs:enumeration value="NSinS"/>
										<xs:enumeration value="DEM"/>
										<xs:enumeration value="RI"/>
										<xs:enumeration value="ESC"/>
										<xs:enumeration value="CD"/>
