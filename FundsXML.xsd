									<xs:restriction base="xs:string">
										<xs:enumeration value="OPEN"/>
										<xs:enumeration value="CLOSE"/>
										<xs:enumeration value="CASH"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Bonds">
