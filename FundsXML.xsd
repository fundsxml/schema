		<xs:annotation>
			<xs:documentation>Frequencys</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="DAIL"/>
					<altova:example value="MNTH"/>
					<altova:example value="QURT"/>
					<altova:example value="SEMI"/>
					<altova:example value="TOMN"/>
					<altova:example value="TOWK"/>
					<altova:example value="TWMN"/>
					<altova:example value="TWWK"/>
					<altova:example value="WEEK"/>
					<altova:example value="YEAR"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
