               <xs:appinfo>
                  <altova:exampleValues>
                     <altova:example value="WBAH"/>
                     <altova:example value="EGSI"/>
                  </altova:exampleValues>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" name="Issuer" type="CompanyType">
